<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ff="clr-namespace:famousfront"
             xmlns:ctrl="clr-namespace:famousfront.controls"
             xmlns:utils="clr-namespace:famousfront.utils"
             xmlns:vm="clr-namespace:famousfront.viewmodels"
             xmlns:metro="http://schemas.codeplex.com/elysium"
             xmlns:params="http://schemas.codeplex.com/elysium/params" 
             xmlns:Primitives="clr-namespace:Elysium.Controls.Primitives;assembly=Elysium" x:Class="famousfront.designtime.Prototype"
             d:DesignWidth="600" 
             params:Design.Theme="Light"      
             mc:Ignorable="d" d:DesignHeight="200" Background="white">
  <UserControl.Resources>
  </UserControl.Resources>
  <Grid x:Name="LayoutRoot">
    <StackPanel>
      <StackPanel Orientation="Horizontal">
      <metro:DropDownCommandButton Content="&#xe015;" TextElement.FontFamily="Segoe UI Symbol" Padding="0" Style="{DynamicResource DropDownCommandButtonStyle1}" TextElement.FontSize="9pt" BorderThickness="0">
        <metro:DropDownCommandButton.Submenu>
          <metro:Submenu>
            <MenuItem Header="Light theme"/>
            <MenuItem Header="Dark theme"/>
          </metro:Submenu>
        </metro:DropDownCommandButton.Submenu>
      </metro:DropDownCommandButton>
        <TextBlock Text="&#xe015;" FontFamily="Segoe UI Symbol" VerticalAlignment="Top" TextElement.FontSize="9pt"/>
        
      </StackPanel>
    </StackPanel>
  </Grid>
</UserControl>
